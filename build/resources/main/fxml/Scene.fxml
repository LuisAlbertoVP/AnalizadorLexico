<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="borderPanePrincipal" maxHeight="680.0" maxWidth="1080.0" minHeight="680.0" minWidth="1080.0" prefHeight="680.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.velasteguicorps.analizador_lexico.Controller.FXMLMenu">
   <top>
      <MenuBar maxHeight="30.0" maxWidth="1080.0" minHeight="30.0" minWidth="1080.0" prefHeight="30.0" prefWidth="1080.0" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Archivo">
            <items>
                  <MenuItem fx:id="menuItemAbrir" mnemonicParsing="false" onAction="#handleArchivo" text="Abrir" />
                  <SeparatorMenuItem mnemonicParsing="false" />
              <MenuItem fx:id="menuItemSalir" mnemonicParsing="false" onAction="#handleArchivo" text="Salir" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Ayuda">
            <items>
              <MenuItem fx:id="menuItemSobre" mnemonicParsing="false" onAction="#handleSobre" text="Sobre" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
